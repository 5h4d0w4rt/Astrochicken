{
  "editor.autoIndent": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.renderControlCharacters": true,
  "editor.renderFinalNewline": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.renderIndentGuides": true,
  "editor.autoClosingQuotes": "languageDefined",
  "python.pythonPath": "/usr/local/bin/python3.7",
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    "--style",
    "{ based_on_style: google, indent_dictionary_value: true }"
  ],
  "python.linting.flake8Args": ["--max-line-length=120"],
  "shellformat.effectLanguages": ["shellscript"],
  "shellcheck.enable": true,
  "shellcheck.executablePath": "shellcheck",
  "shellcheck.run": "onSave",
  "shellcheck.customArgs": [
    "--shell=bash",
    "--severity=warning",
    "--check-sourced",
    "--wiki-link-count=2"
  ],
  "python.linting.pylintUseMinimalCheckers": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.banditEnabled": true,
  "python.linting.maxNumberOfProblems": 100,
  "python.analysis.cacheFolderPath": "~/Library/Application Support/Code/.__python_analysis_cache",
  "python.testing.pytestEnabled": true,
  "python.testing.pytestArgs": ["--exitfirst", "--verbose"],
  "python.testing.pytestPath": "pytest",
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.promptToConfigure": true,
  "python.testing.unittestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "gitlens.insiders": true,
  "git.autofetch": true,
  "git.confirmForcePush": true,
  "git.inputValidation": "always",
  "git.showProgress": true,
  "git.autoStash": true,
  "git.openDiffOnClick": true,
  "liveshare.allowGuestTaskControl": true,
  "liveshare.allowGuestDebugControl": true,
  "liveshare.anonymousGuestApproval": "reject",
  "liveshare.focusBehavior": "prompt",
  "liveshare.featureSet": "insiders",
  "liveshare.autoShareTerminals": true,
  "liveshare.keepAliveInterval": 10,
  "liveshare.autoShareServers": true,
  "liveshare.increasedGuestLimit": true,
  "liveshare.languages.allowGuestCommandControl": true,
  "liveshare.populateGitCoAuthors": "always",
  "liveshare.codeLens": true,
  "liveshare.shareExternalFiles": true,
  "chat.autoLaunchLiveShareChat": false,
  "markdownlint.run": "onSave",
  "diffEditor.ignoreTrimWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "afterDelay",
  "telemetry.enableTelemetry": false,
  "liveshare.connectionMode": "auto",
  "yaml.format.enable": true,
  "json.format.enable": true,
  "terraform.telemetry.enabled": false,
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/~/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.minikube-path.mac": "/Users/$USER/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.helm-path.mac": "/Users/$USER/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.draft-path.mac": "/Users/$USER/.vs-kubernetes/tools/draft/darwin-amd64/draft"
  },
  "eslint.enable": true,
  "eslint.options": {
    "extends": ["eslint:recommended", "google"]
  },
  "eslint.run": "onSave",
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "git.suggestSmartCommit": false,
  "liveshare.accountProvider": "github"
}
